<% var title = 'Calendrier' %>
<% include head %>

<h1>Liste des dates</h1>
<%- message %>


<ul>
  <% dates.forEach(function(noplanningdate) { %>
  <li>
    <%= noplanningdate.date %>
  </li>
  <% }); %>
</ul>

<form action="/date" method="post">
  <p>
    <label>Date :</label>
    <input type="text" name="date">
  </p>
  <p>
    <input type="submit" value="Valider">
  </p>
</form>
<% include foot %>